<h1>All your bookings!</h1>

<% @creature.bookings.each do |booking| %>
  <li> <%=booking.start_time  %> - <%= booking.end_time %> <%= link_to 'See details', booking_path(booking) %></li>
  <li> <%= link_to 'Edit', edit_creature_booking_path(booking) %> <%= link_to 'Destroy', booking_path(booking), method: :delete %></li>
<% end %>
